.TH sfxtomo 1  "APRIL 2019" Madagascar "Madagascar Manuals"
.SH NAME
sfxtomo \- Kjartansson-style tomography 
.SH SYNOPSIS
.B sfxtomo < inp.rsf > out.rsf adj=y nz=nh oz=oh dz=dh nx=ny ox=oy dx=dy nh=nz oh=oz dh=dz ny=nx oy=ox dy=dx niter=-1
.SH PARAMETERS
.PD 0
.TP
.I bool   
.B adj
.B =y
.R  [y/n]	adjoint flag
.TP
.I float  
.B dh
.B =dz
.R  
.TP
.I float  
.B dx
.B =dy
.R  
.TP
.I float  
.B dy
.B =dx
.R  
.TP
.I float  
.B dz
.B =dh
.R  
.TP
.I int    
.B nh
.B =nz
.R  
.TP
.I int    
.B niter
.B =-1
.R  	number of iterations
.TP
.I int    
.B nx
.B =ny
.R  
.TP
.I int    
.B ny
.B =nx
.R  
.TP
.I int    
.B nz
.B =nh
.R  
.TP
.I float  
.B oh
.B =oz
.R  
.TP
.I float  
.B ox
.B =oy
.R  
.TP
.I float  
.B oy
.B =ox
.R  
.TP
.I float  
.B oz
.B =oh
.R  
.SH SOURCE
.I user/gee/Mxtomo.c
.SH VERSION
2.1-git
