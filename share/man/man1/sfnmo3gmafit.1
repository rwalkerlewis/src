.TP
.I file   
.B fit
.B =
.R  	auxiliary output file name
.TP
.I bool   
.B gather
.B =n
.R  [y/n]	Memory allocation
.TP
.I file   
.B offsetx
.B =
.R  	auxiliary input file name
.TP
.I file   
.B offsety
.B =
.R  	auxiliary input file name
.TP
.I bool   
.B verb
.B =n
.R  [y/n]
.SH SOURCE
.I user/zone/Mnmo3gmafit.c
.SH VERSION
2.1-git
