.TH sfkirdat 1  "APRIL 2019" Madagascar "Madagascar Manuals"
.SH NAME
sfkirdat \- 2-D Pre-stack Kirchhoff redatuming. 
.SH SYNOPSIS
.B sfkirdat < in.rsf > out.rsf sgreen=sgreen.rsf rgreen=rgreen.rsf interm=interm.rsf verb=n datum= aperture=50 taper=10 length=0.025
.SH PARAMETERS
.PD 0
.TP
.I int    
.B aperture
.B =50
.R  	aperture (number of traces)
.TP
.I float  
.B datum
.B =
.R  	datum depth
.TP
.I string 
.B interm
.B =
.R  	auxiliary output file name
.TP
.I float  
.B length
.B =0.025
.R  	filter length (in seconds)
.TP
.I file   
.B rgreen
.B =
.R  	auxiliary input file name
.TP
.I file   
.B sgreen
.B =
.R  	auxiliary input file name
.TP
.I int    
.B taper
.B =10
.R  	taper (number of traces)
.TP
.I bool   
.B verb
.B =n
.R  [y/n]	verbosity flag
.SH SOURCE
.I user/llisiw/Mkirdat.c
.SH VERSION
2.1-git
