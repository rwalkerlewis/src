.TP
.I string 
.B receiver
.B =
.R  	auxiliary input file name
.TP
.I string 
.B record
.B =
.R  	auxiliary input file name
.TP
.I string 
.B source
.B =
.R  	auxiliary input file name
.TP
.I int    
.B uts
.B =0
.R  
.SH SOURCE
.I user/hzhu/Mhelm2D_rtm.c
