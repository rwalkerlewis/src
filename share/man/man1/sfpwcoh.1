.TH sfpwcoh 1  "APRIL 2019" Madagascar "Madagascar Manuals"
.SH NAME
sfpwcoh \- Coherency by plane-wave construction. 
.SH SYNOPSIS
.B sfpwcoh < in.rsf > out.rsf dip=dip.rsf a2=a2.rsf b2=b2.rsf verb=n eps=0.01 rect=2 order=1
.SH PARAMETERS
.PD 0
.TP
.I file   
.B a2
.B =
.R  	auxiliary output file name
.TP
.I file   
.B b2
.B =
.R  	auxiliary output file name
.TP
.I file   
.B dip
.B =
.R  	auxiliary input file name
.TP
.I float  
.B eps
.B =0.01
.R  	regularization
.TP
.I int    
.B order
.B =1
.R  	accuracy order
.TP
.I int    
.B rect
.B =2
.R  	spread
.TP
.I bool   
.B verb
.B =n
.R  [y/n]
.SH USED IN
.TP
.I tccs/flat/flat
.SH SOURCE
.I user/pwd/Mpwcoh.c
